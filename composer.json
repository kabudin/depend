{
  "name": "zeno/depend",
  "description": "An annotation declaration that adapts to the hyperf framework to cover dependency relationships",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "depend"
  ],
  "autoload": {
    "psr-4": {
      "Zeno\\Depend\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/di": "^3.1",
    "hyperf/event": "^3.1",
    "hyperf/framework": "^3.1"
  },
  "suggest": {},
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.1-dev"
    },
    "hyperf": {
      "config": "Zeno\\Depend\\ConfigProvider"
    }
  }
}
